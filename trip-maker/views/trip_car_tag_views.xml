<odoo>
    <record id="trip_car_tag_action" model="ir.actions.act_window">
        <field name="name">Car Tags</field>
        <field name="res_model">trip.car.tag</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
          <p class="o_view_nocontent_smiling_face">
            No record
          </p>
        </field>
    </record>
    <record id="trip_car_tag_view_tree" model="ir.ui.view">
        <field name="name">trip.car.tag</field>
        <field name="model">trip.car.tag</field>
        <field name="arch" type="xml">
            <tree string="Channel" expand="1" editable="bottom">
                <field name="name"/> 
            </tree>
        </field>
    </record>
    <record id="trip_car_tag_view_form" model="ir.ui.view">
      <field name="name">trip.car.tag</field>
        <field name="model">trip.car.tag</field>
        <field name="arch" type="xml">
          <form>
            <sheet>
              <div class="oe_edit_only">
                <label for="name"/>
              </div>
              <h1>
                <field name="name" />
              </h1>
            </sheet>
          </form>
        </field>
    </record>
</odoo>